                    if (window['turnstileRetry'] <= window['maxRetries']) {
                        turnstile.reset(widgetid);
                        turnstile.execute(); 
                    } else {
                        unhideProblem(null);
                    }
                } catch(error) {
                    unhideProblem(error);
                }
            }
            
